{
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@inabagumi/eslint-config-react": "^9.0.1",
    "@inabagumi/prettier-config": "^2.0.0",
    "@types/react": "^18.2.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ],
    "root": true
  },
  "packageManager": "yarn@1.22.19",
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "resolutions": {
    "cheerio": "^1.0.0-rc.12",
    "trim": "^1.0.1"
  },
  "scripts": {
    "build": "yarn workspace @manael/website build",
    "format": "prettier --write './**/*.{json,yml}'",
    "format-check": "prettier -c './**/*.{json,yml}'",
    "lint": "eslint .",
    "prepare": "is-ci || husky install"
  },
  "workspaces": [
    "website"
  ]
}
