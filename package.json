    "@commitlint/config-conventional": "^19.4.1",
    "@inabagumi/prettier-config": "^3.0.0",
    "@types/react": "^18.3.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.5",
    "is-ci": "^3.0.1",
