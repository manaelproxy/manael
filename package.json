{
  "devDependencies": {
    "@inabagumi/eslint-config-react": "^6.0.1",
    "@inabagumi/prettier-config": "^1.1.2",
    "@types/react": "^16.9.49",
    "eslint": "^7.9.0",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": "12.x"
  },
  "eslintConfig": {
    "extends": [
      "@inabagumi/react"
    ],
    "root": true
  },
  "prettier": "@inabagumi/prettier-config",
  "private": true,
  "scripts": {
    "build": "yarn workspace @manael/website build",
    "format": "prettier --ignore-path .gitignore --write .",
    "format-check": "prettier -c --ignore-path .gitignore .",
    "lint": "eslint ."
  },
  "workspaces": [
    "website"
  ]
}
